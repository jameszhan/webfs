<!DOCTYPE html>
<html>
<head>
  <title>Webfs</title>
  <%= stylesheet_link_tag    "application", media: "all", "data-turbolinks-track" => true %>
  <%= javascript_include_tag "application", "data-turbolinks-track" => true %>
  <%= csrf_meta_tags %>
</head>
<body>
	<header class="navbar navbar-inverse navbar-fixed-top" role="banner">
	    <div class="container">
	        <div class="navbar-header">
	            <button class="navbar-toggle" type="button" data-toggle="collapse" data-target=".bs-navbar-collapse">
	                <span class="sr-only">WebFS</span>
	                <span class="icon-bar"></span>
	                <span class="icon-bar"></span>
	                <span class="icon-bar"></span>
	            </button>
	            <a href="/box/index.htm" class="navbar-brand">WebFS</a>
	        </div>
	        <nav class="collapse navbar-collapse bs-navbar-collapse" role="navigation">
	            <ul class="nav navbar-nav">
	                <li><a href="/box/index.htm">1</a></li>
	                <li><a href="/box/new_supplier.htm">2</a></li>
	                <li><a href="javascript:void(0);" data-toggle="modal" data-target="#postalReference">3</a></li>
	                <li><a href="javascript:void(0);" data-toggle="modal" data-target="#aircraftReference">4</a></li>
	                <li><a href="/box/auto_pricing.htm">5</a></li>
	            </ul>
	            <ul class="nav navbar-nav navbar-right">
	                <li>
	                    <a href="javascript:void(0);">用户管理</a>
	                </li>
	            </ul>
	        </nav>
	    </div>
	</header>
	
	<div class="container">
	<%= yield %>
	</div>

	<!-- Modal -->
	<div class="modal fade" id="previewModal" tabindex="-1" role="dialog" aria-labelledby="previewLabel" aria-hidden="true">
	  <div class="modal-dialog">
	    <div class="modal-content">
	      <div class="modal-header">
	        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
	        <h4 class="modal-title" id="previewLabel">Modal title</h4>
	      </div>
	      <div class="modal-body" style="padding: 0;">
            <iframe style="height:100%;width:100%;min-height:500px;margin:0;padding:0;border:none;"></iframe>
	      </div>
	      <div class="modal-footer">
					<button type="button" class="btn btn-danger btn-xs open-file" data-dismiss="modal">Open</button>
	        <button type="button" class="btn btn-default btn-xs" data-dismiss="modal">Close</button>
	      </div>
	    </div><!-- /.modal-content -->
	  </div><!-- /.modal-dialog -->
	</div><!-- /.modal -->

</body>
</html>
